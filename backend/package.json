        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "eslint": "^8.56.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.1",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "supertest": "^6.3.4"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageThreshold": {
